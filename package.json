{
  "name": "N3-components",
  "version": "2.0.0",
  "description": "N3 built with Vue.js",
  "main": "dist/N3Components.min.js",
  "homepage": "https://n3-components.github.io/N3-components/index.html",
  "repository": {
    "type": "git",
    "url": "https://github.com/N3-components/N3-components"
  },
  "directories": {
    "src": "src/"
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "node": true
    }
  },
  "dependencies": {
    "velocity-animate": "^1.2.3",
    "vue-focus": "^2.1.0"
  },
  "files": [
    "LICENSE",
    "README.md",
    "CHANGELOG.md",
    "src",
    "dist"
  ],
  "keywords": [
    "vue",
    "n3",
    "vue-component"
  ],
  "devDependencies": {
    "autoprefixer": "^6.5.1",
    "babel-core": "^6.1.21",
    "babel-loader": "^6.1.0",
    "babel-plugin-transform-runtime": "^6.1.18",
    "babel-polyfill": "^6.13.0",
    "babel-preset-es2015": "^6.1.18",
    "css-loader": "^0.21.0",
    "eslint-config-standard": "^5.1.0",
    "eslint-friendly-formatter": "^1.2.2",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-html": "^1.5.3",
    "eslint-plugin-promise": "^1.0.8",
    "eslint-plugin-standard": "^2.0.1",
    "extract-text-webpack-plugin": "~1.0.1",
    "file-loader": "~0.9.0",
    "ghooks": "^1.3.2",
    "less": "^2.5.3",
    "less-loader": "^2.2.2",
    "postcss-loader": "^1.1.0",
    "prismjs": "^1.3.0",
    "style-loader": "^0.13.0",
    "url-loader": "~0.5.7",
    "validate-commit-msg": "^2.8.2",
    "vue": "^2.0.3",
    "vue-hot-reload-api": "^1.2.0",
    "vue-html-loader": "^1.0.0",
    "vue-loader": "7.1.7",
    "webpack": "^1.12.2",
    "webpack-dev-server": "^1.12.0"
  },
  "browserify": {
    "transform": [
      [
        "vueify"
      ]
    ]
  },
  "scripts": {
    "dev": "webpack --watch & npm run docs",
    "docs": "webpack-dev-server --inline --hot --quiet --content-base docs",
    "build": "webpack --progress --hide-modules --config  webpack.build.min.js && webpack --progress --hide-modules --config  webpack.build.js",
    "lint": "eslint --ext .js,.vue src"
  },
  "author": "zhangking",
